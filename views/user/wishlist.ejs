                    <span></span> Shop
                    <span></span> Wishlist
                </div>
            </div>
        </div>
        <section class="mt-50 mb-50">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="table-responsive">
                            <table class="table shopping-summery text-center">
                                <thead>
                                    <tr class="main-heading">
                                        <th scope="col" colspan="2">Product</th>
                                        <th scope="col">Name</th>
                                        <th scope="col">Price</th>
                                        <th scope="col">Action</th>
                                      
                                    </tr>
                                </thead>
                                <tbody id="cart-items">
                                    <!-- Use EJS to loop through cart items -->
                                    <% products.forEach(function(product,index) { %>
                                        <tr>
                                            <td><%=index+1%></td>
                                            <td>
                                                <!-- Display product image here -->
                                                <img src="productimages/<%= product.ProductDetails[0].image[0] %>" alt="<%= product.image %>">
                                            </td>
                                            <td>
                                                <!-- Display product name here -->
                                                <%= product.ProductDetails[0].productname %>
                                            </td>
                                            <td>
                                                <!-- Display product price here -->
                                                <%= product.ProductDetails[0].saleprice %> 
                                            </td>
                                            <td>
                                                <button type="submit" class="button button-add-to-cart" onclick="addToCart('<%= product.ProductDetails[0]._id %>' , '<%= product.ProductDetails[0].saleprice %>')">Add to cart</button>

                                            </td>
                            
                                        
                                           
                                            <td>
                                                <!-- Add a button/link to remove the item -->
                                                <a href="/removeFromWishlist/<%= product.ProductDetails[0]._id %>" class="btn btn-danger">Remove</a>
                                            </td>
                                        </tr>
                                    <% }); %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </main>
