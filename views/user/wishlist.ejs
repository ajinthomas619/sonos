    <script>
        function addToCart(proId,price){
              $.ajax({
                  url:'/add-to-cart',
                  method:'post',
                  data:{
                      id:proId,
                      price:price,
                  },
                  success:(response)=>{
                      if(response == true){
                          Swal.fire({
                              icon: 'success',
                              title: 'Added to cart',
                              text: 'Product added to cart succesfully',
                          })
                      }
                  }
              })
          }
          </script>
